{
    "name": "sbine/route-viewer",
    "description": "A Laravel Nova tool to view your registered routes.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "routes"
    ],
    "authors": [
        {
            "name": "Sara Bine",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/sbine/nova-route-viewer",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0",
        "laravel/nova": "^4.0 || ^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Sbine\\RouteViewer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sbine\\RouteViewer\\ToolServiceProvider"
            ]
        }
    }
}
